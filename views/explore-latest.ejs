<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">Explore More</li>
  </ol>
</nav>

<h1 class="pb-4">Latest Additions</h1>

<div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">
  <% if(recipe != '') { %> <% recipe.forEach(function(recipe, index) { %>
  <a href="/recipe/<%= recipe._id %>" class="col text-center category__Link">
    <div class="category__img--Large shadow">
      <img
        src="uploads/<%= recipe.image %>"
        alt="<%= recipe.name %>"
        Loading="lazy"
      />
    </div>
    <div class="pt-1"><%= recipe.name %></div></a
  >
  <% }) %> <% } %>
</div>
